<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageId>netcore-sci</PackageId>
        <PackageVersion>1.15.1</PackageVersion>
        <Title>net core scientific</Title>
        <Description></Description>
        <LangVersion>8</LangVersion>
        <Authors>Lorenzo Delana</Authors>
        <PackageTags>netcore scientific linear-algebra measure units-of-measure 3d polygon dxf quaternion</PackageTags>
        <PackageProjectUrl>https://github.com/devel0/netcore-sci#netcore-sci</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <noWarn>1591</noWarn>
    </PropertyGroup>    
    <ItemGroup>
        <PackageReference Include="LibTessDotNet" Version="1.1.13" />
        <PackageReference Include="LinqStatistics" Version="2.3.0" />
        <PackageReference Include="MathNet.Numerics" Version="4.12.0" />        
        <PackageReference Include="netcore-psql-util" Version="1.2.1" />
        <!-- <PackageReference Include="netcore-util" Version="1.6.2" /> -->        
        <PackageReference Include="netDxf.netstandard" Version="2.3.0" />        
        <PackageReference Include="ParagonClipper" Version="6.4.2" />
        <PackageReference Include="QuantumConcepts.Formats.STL.netcore" Version="1.3.1" />
    </ItemGroup>    
    <ItemGroup>
      <ProjectReference Include="..\..\netcore-util\netcore-util\netcore-util.csproj" />
    </ItemGroup>   
</Project>