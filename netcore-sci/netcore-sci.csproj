<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageId>netcore-sci</PackageId>
        <PackageVersion>1.14.0</PackageVersion>
        <Title>net core scientific</Title>
        <Description></Description>
        <LangVersion>8</LangVersion>
        <Authors>Lorenzo Delana</Authors>
        <PackageTags>netcore scientific linear-algebra measure units-of-measure 3d polygon dxf quaternion</PackageTags>
        <PackageProjectUrl>https://github.com/devel0/netcore-sci#netcore-sci</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <noWarn>1591</noWarn>
    </PropertyGroup>    
    <ItemGroup>
        <PackageReference Include="LinqStatistics" Version="2.3.0" />        
        <PackageReference Include="netcore-psql-util" Version="1.2.1" />
        <PackageReference Include="netcore-util" Version="1.6.2" />
        <PackageReference Include="netDXF.Standard" Version="2.1.1" />
        <PackageReference Include="ParagonClipper" Version="6.4.2" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Avalonia" Version="0.10.0-preview2" />
        <PackageReference Include="Avalonia.Desktop" Version="0.10.0-preview2" />
        <PackageReference Include="Avalonia.ReactiveUI" Version="0.10.0-preview2" />
        <PackageReference Include="QuantumConcepts.Formats.STL.netcore" Version="1.3.1" />
    </ItemGroup>    
    <ItemGroup>
        <!-- <PackageReference Include="Silk.NET" Version="2.0.0-preview" /> -->
    </ItemGroup>    
</Project>